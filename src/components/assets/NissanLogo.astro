---
import type { HTMLAttributes } from "astro/types";
export type Props = HTMLAttributes<"svg">;
---

<svg viewBox="0 0 128 128" {...Astro.props}>
  <defs>
    <style>
      .cls-1 {
        fill: url(#linear-gradient);
      }

      .cls-2 {
        fill: url(#linear-gradient-2);
      }

      .cls-3 {
        fill: #fff;
      }

      .cls-4 {
        fill: #494949;
      }
    </style>
    <linearGradient
      id="linear-gradient"
      x1="64"
      x2="64"
      y2="128"
      gradientUnits="userSpaceOnUse"
    >
      <stop offset="0" stop-color="#484848"></stop>
      <stop offset="0.5" stop-color="#3c3c3c"></stop>
      <stop offset="1" stop-color="#383838"></stop>
    </linearGradient>
    <linearGradient
      id="linear-gradient-2"
      x1="64"
      y1="28"
      x2="64"
      y2="100"
      gradientUnits="userSpaceOnUse"
    >
      <stop offset="0" stop-color="#303030"></stop>
      <stop offset="0.42" stop-color="#282828"></stop>
      <stop offset="1" stop-color="#242424"></stop>
    </linearGradient>
  </defs>
  <g id="Background">
    <g id="_Rectangle_">
      <rect class="cls-1" width="128" height="128"></rect>
    </g>
  </g>
  <g id="Foreground">
    <rect class="cls-2" x="28" y="28" width="72" height="72" rx="8"></rect>
    <g id="N_Dots">
      <circle class="cls-3" cx="49" cy="49" r="3"></circle>
      <circle class="cls-3" cx="49" cy="59" r="3"></circle>
      <circle class="cls-3" cx="49" cy="69" r="3"></circle>
      <circle class="cls-3" cx="49" cy="79" r="3"></circle>
      <circle class="cls-4" cx="59" cy="49" r="3"></circle>
      <circle class="cls-3" cx="59" cy="59" r="3"></circle>
      <circle class="cls-4" cx="59" cy="69" r="3"></circle>
      <circle class="cls-4" cx="59" cy="79" r="3"></circle>
      <circle class="cls-4" cx="69" cy="49" r="3"></circle>
      <circle class="cls-4" cx="69" cy="59" r="3"></circle>
      <circle class="cls-3" cx="69" cy="69" r="3"></circle>
      <circle class="cls-4" cx="69" cy="79" r="3"></circle>
      <circle class="cls-3" cx="79" cy="49" r="3"></circle>
      <circle class="cls-3" cx="79" cy="59" r="3"></circle>
      <circle class="cls-3" cx="79" cy="69" r="3"></circle>
      <circle class="cls-3" cx="79" cy="79" r="3"></circle>
    </g>
  </g>
</svg>
